<!-- Drug master data -->
<odoo>
    <record id="drug_amoxicillin" model="medical.drug">
        <field name="name">Amoxicillin</field>
        <field name="code">AMX-500</field>
        <field name="default_dosage">500 mg</field>
        <field name="default_frequency">Three times daily</field>
        <field name="default_duration">7 days</field>
    </record>

    <record id="drug_ibuprofen" model="medical.drug">
        <field name="name">Ibuprofen</field>
        <field name="code">IBU-200</field>
        <field name="default_dosage">200 mg</field>
        <field name="default_frequency">Every 6-8 hours as needed</field>
        <field name="default_duration">5 days</field>
    </record>

    <record id="drug_hydrocortisone" model="medical.drug">
        <field name="name">Hydrocortisone Cream 1%</field>
        <field name="code">HDC-1P</field>
        <field name="default_dosage">Apply thin layer</field>
        <field name="default_frequency">Twice daily</field>
        <field name="default_duration">7 days</field>
    </record>
</odoo>