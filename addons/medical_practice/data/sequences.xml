<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Patient ID Sequence -->
        <record id="seq_medical_patient" model="ir.sequence">
            <field name="name">Medical Patient</field>
            <field name="code">medical.patient</field>
            <field name="prefix">PAT-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Appointment Sequence -->
        <record id="seq_medical_appointment" model="ir.sequence">
            <field name="name">Medical Appointment</field>
            <field name="code">medical.appointment</field>
            <field name="prefix">APP-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Medical Record Sequence -->
        <record id="seq_medical_record" model="ir.sequence">
            <field name="name">Medical Record</field>
            <field name="code">medical.record</field>
            <field name="prefix">MR-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Prescription Sequence -->
        <record id="seq_medical_prescription" model="ir.sequence">
            <field name="name">Medical Prescription</field>
            <field name="code">medical.prescription</field>
            <field name="prefix">RX-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>